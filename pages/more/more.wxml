<!--pages/more/more.wxml-->
<view class="wrapper">
  <view class="entry {{'dress-' + index}}" 
    wx:for="{{entries}}" 
    wx:key="index"
    data-url="{{item.url}}"
    bindtap="jumpTo">
    <text>{{item.name}}</text>
  </view>
</view>
<!-- version -->
<view class="version-wrapper">
  <text bindtap="versionTap" class="version">V1.0.8</text>
  <text class="tip">该项目以后随缘更新</text>
  <text class="tip">新项目<text class="stitches">Stitches</text>缓慢开发中...</text>
  <text class="tip">唯一指定微博：<text class="weibo">热搜分析师bot</text></text>
  <text class="tip">联系邮箱：<text class="email">1031568754@qq.com</text></text>
</view>

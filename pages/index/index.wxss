page {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f8f8f8;
}

.content-wrapper {

}
.update {
  position: fixed;
  width: 100%;
  text-align: center;
  color: #939393;
  font-size:30rpx;
  margin-top:20rpx;
  z-index: -1;
}
.content {

}

.item-wrapper {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-bottom:1px solid #e6e6e6;
  background-color:#fff;
}
.item-index {
  font-size: 40rpx;
  width: 100rpx;
  text-align: center;
  font-style: italic;
}
.top-index {
  color: #ff576b;
  font-weight: bold;
}
.other-index {
  color: #ffb642;
}
.item-desc {
  font-size: 38rpx;
  flex-grow: 1;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  padding-right: 20rpx;
}
.item-n {
  font-size: 30rpx;
  color: #939393;
  padding-right:40rpx;
}

.tip {
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #999;
}

.icon-nodata {
  font-size: 500rpx;
  margin-top: 100rpx;
}


